	<model name="Sea_level_rise_costs_and_impacts">
		<variables>
			<aux name="Total SLR protection costs" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Total_investment_in_fp + Total_net_SLR_fp_maintenance_costs</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Total fp maintenance costs">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Annual_costs_of_flood_protection_maintenance[Run,well_protected] + Annual_costs_of_flood_protection_maintenance[Run,insufficient]</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Total investment in fp" isee:derived_units="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Effective_annual_investment_in_flood_protection_height[Run,well_protected] + Effective_annual_investment_in_flood_protection_height[Run,insufficient]</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Total reference fp maintenance costs">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Reference_maintenance_costs_without_adaptation[Run,well_protected] + Reference_maintenance_costs_without_adaptation[Run,insufficient]</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="global death rate from coastal flooding" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Total_fatalities_due_to_coastal_floods/Population</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Total fatalities due to coastal floods" isee:derived_units="true">
				<eqn>SUM(Annual_fatalities_due_to_coastal_floods[*, well_protected]) + SUM(Annual_fatalities_due_to_coastal_floods[*, insufficient])</eqn>
				<isee:summing/>
				<units>Mp/year</units>
			</aux>
			<aux name="Mobile assets\nfraction">
				<doc>As in CIAM: 75% of capital are immobile</doc>
				<eqn>0.25</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Cost to demolish\nabandoned assets" isee:derived_units="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Asset_demolition_cost_factor*(1-Mobile_assets_fraction)*(Annual_retreat_of_coastal_assets)</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Asset relocation cost factor">
				<eqn>0.1</eqn>
				<units>1</units>
			</aux>
			<aux name="Asset demolition cost factor">
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<aux name="Asset relocation costs">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Asset_relocation_cost_factor*(Annual_retreat_of_coastal_assets) * Mobile_assets_fraction</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Annual retreat\nof coastal assets" isee:derived_units="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Proactive_retreat_of_coastal_assets + SLR_driven_retreat_of_coastal_assets_1</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Loss of assets\nduring inundation">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>(1.0 - Mobile_assets_fraction) *SLR_driven_retreat_of_coastal_assets_1</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Annual\ncosts of\nflood protection\nmaintenance" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Reference maintenance costs without adaptation" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Effective annual\ninvestment in flood protection height" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Increase factor for costs of reactive retreat">
				<eqn>5</eqn>
				<units>1</units>
			</aux>
			<aux name="Proactive retreat of people from the coast 1" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp/year</units>
			</aux>
			<aux name="People retreat costs" access="output" isee:autocreated="true">
				<doc>Relocating people costs 1x their annual income</doc>
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>(Proactive_retreat_of_people_from_the_coast_1+Increase_factor_for_costs_of_reactive_retreat*SLR_driven_retreat_of_people_from_the_coast_1)*GDP_per_capita_in_coastal_zones</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="GDP per capita\nin coastal zones" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/Mp</units>
			</aux>
			<aux name="SLR driven retreat of people from the coast 1" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp/year</units>
			</aux>
			<aux name="Total people retreat costs">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(People_retreat_costs[Run, *])</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Annual fatalities due to\ncoastal floods" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp/year</units>
			</aux>
			<aux name="Number of\npeople flooded\nannually in storms" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp/year</units>
			</aux>
			<aux name="Proactive retreat\nof coastal assets" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="SLR driven retreat of coastal assets 1" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Total asset retreat costs">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(Asset_relocation_costs[Run, *]) + SUM(Cost_to_demolish_abandoned_assets[Run, *])</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Total relocation cost">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Total_asset_retreat_costs + Total_people_retreat_costs</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Total flood costs">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(Loss_of_assets_during_inundation[Run,*]) + SUM(Loss_of_assets_during_retreat[Run, *]) + SUM(Opportunity_costs_of_inundated_coastal_land[Run,*])</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Opportunity costs of\ninundated coastal land" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Loss of assets\nduring retreat">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Proactive_retreat_of_coastal_assets*(1.0 - Mobile_assets_fraction) * Not_depreciated_fraction_of_assets_at_time_of_retreat</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Not depreciated fraction of assets at time of retreat">
				<eqn>0.1</eqn>
				<units>1</units>
			</aux>
			<aux name="Total construct costs">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(Annual_opportunity_costs_of_flood_protection[Run,*]) + Total_investment_in_fp + Total_net_SLR_fp_maintenance_costs</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Annual opportunity costs of flood protection" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Total opportunity costs of SLR">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(Annual_opportunity_costs_of_flood_protection[Run, *]) + SUM(Opportunity_costs_of_inundated_coastal_land[Run, *])</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="All storm damage" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Total asset storm costs">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(All_storm_damage[Run, *])</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Total net SLR fp maintenance costs">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Total_fp_maintenance_costs-Total_reference_fp_maintenance_costs</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Population" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp</units>
			</aux>
			<aux name="inflation adjustment b$ per bc$ 1" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>b$/bc$</units>
			</aux>
			<aux name="Total nominal SLR\nprotection cost" access="output">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Total_SLR_protection_costs*&quot;inflation_adjustment_b$_per_bc$_1&quot;</eqn>
				<units>b$/year</units>
			</aux>
			<aux name="Total nominal opportunity costs of SLR">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Total_opportunity_costs_of_SLR*&quot;inflation_adjustment_b$_per_bc$_1&quot;</eqn>
				<units>b$/year</units>
			</aux>
			<aux name="Total number of people flooded">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(Number_of_people_flooded_annually_in_storms[*, *])</eqn>
				<isee:summing/>
				<units>Mp/year</units>
			</aux>
			<aux name="Proportion of working\npopulation affected by flooding">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Time_over_which_people_are_flooded*Total_number_of_people_flooded*(Employed/Population_1) / Employed</eqn>
				<units>1</units>
			</aux>
			<aux name="Time over which people are flooded">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Productivity reduction\nfactor from coastal flooding" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>1 - Proportion_of_working_population_affected_by_flooding*Relative_productivity_loss_of_people_being_flooded</eqn>
				<units>1</units>
			</aux>
			<aux name="Relative productivity loss of people being flooded">
				<eqn>0.96</eqn>
				<units>1</units>
			</aux>
			<aux name="Population 1" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp</units>
			</aux>
			<stock name="Employed" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<non_negative/>
				<units>Mp</units>
			</stock>
			<aux name="Total loss of assets from SLR">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(Loss_of_assets_during_inundation[Run,*]) + SUM(Loss_of_assets_during_retreat[Run,*]) + Total_storm_surge_damages</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Total storm surge damages" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SUM(All_storm_damage[Run, *])</eqn>
				<isee:summing/>
				<units>bc$/year</units>
			</aux>
			<aux name="Total nominal loss of assets from SLR" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Total_loss_of_assets_from_SLR*&quot;inflation_adjustment_b$_per_bc$&quot;</eqn>
				<units>b$/year</units>
			</aux>
			<aux name="inflation adjustment b$ per bc$" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>b$/bc$</units>
			</aux>
			<aux name="Total nominal people\nretreat costs" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>&quot;inflation_adjustment_b$_per_bc$_1&quot;*Total_people_retreat_costs</eqn>
				<units>b$/year</units>
			</aux>
		</variables>
