			<aux name="Flood damage resilience in unprotected areas" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>1</units>
			</aux>
			<aux name="Flood damage function\nfor people in\ncoastal areas">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>people_flood_damage_sensitivity* (Effective_flood_depth_for_people/UNIT_metre_1)^people_flood_damage_elasticity</eqn>
				<units>1/Year</units>
			</aux>
			<aux name="people flood damage\nsensitivity">
				<eqn>0.5</eqn>
				<units>1/year</units>
			</aux>
			<aux name="people flood damage\nelasticity">
				<eqn>2</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Expected susceptible fraction\nof population in 50 years">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>population_storm_damage_max_fraction[Coastal_Protection_level] / (1.0 + EXP(-population_storm_damage_logistic_growth_rate[Coastal_Protection_level]*(Expected_net_global_flood_height_in_50_years - population_storm_damage_halfpoint[Coastal_Protection_level]) ) )</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction of original people distribution that would experience storm damage">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>population_storm_damage_max_fraction[Coastal_Protection_level] / (1.0 + EXP(-population_storm_damage_logistic_growth_rate[Coastal_Protection_level]*(Net_global_flood_height - population_storm_damage_halfpoint[Coastal_Protection_level]) ) )</eqn>
				<units>1</units>
			</aux>
			<aux name="population storm damage halfpoint">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>-1.38</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>0.98</eqn>
				</element>
				<units>metre</units>
			</aux>
			<aux name="population storm damage logistic growth rate">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>0.48</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>2.69</eqn>
				</element>
				<units>1/metre</units>
			</aux>
			<aux name="UNIT metre 1" access="input">
				<eqn>1</eqn>
				<units>metre</units>
			</aux>
			<stock name="Effective flood depth\nfor people" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>0</eqn>
				<inflow>Change_in_effective_flood_depth_for_people</inflow>
				<units>metre</units>
			</stock>
			<flow name="Change in effective\nflood depth for people">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MAX(-ABS(Effective_flood_depth_for_people),
( ( Effective_flood_depth_for_people + Change_in_net_global_flood_height) * DELAY(Fraction_of_original_people_distribution_that_would_experience_storm_damage, 1) + (Fraction_of_original_people_distribution_that_would_experience_storm_damage - DELAY(Fraction_of_original_people_distribution_that_would_experience_storm_damage,  1))*(0.5*Change_in_net_global_flood_height) ) / Fraction_of_original_people_distribution_that_would_experience_storm_damage   - Effective_flood_depth_for_people)  / Time_for_change_in_flood_depth</eqn>
				<units>metre/year</units>
			</flow>
			<stock name="Inundated fraction\nof original coastal\npopulation distribution">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Initial_inundated_and_retreated_people_fraction[Coastal_Protection_level]</eqn>
				<inflow>relative_annual_inundation_of_population_due_to_SLR</inflow>
				<units>1</units>
			</stock>
			<flow name="relative annual inundation\nof population due to SLR">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MAX(0,  (Population_inundation_max_fraction[Coastal_Protection_level] / (1.0 + EXP(-population_inundation_logistic_growth_rate[Coastal_Protection_level]*(Net_global_flood_height - population_inundation_halfpoint[Coastal_Protection_level]) ) ) - Inundated_fraction_of_original_coastal_population_distribution) / time_over_which_coastal_people_retreat )</eqn>
				<units>1/year</units>
			</flow>
			<aux name="population inundation halfpoint">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>2.16</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>2.4</eqn>
				</element>
				<units>metre</units>
			</aux>
			<aux name="population inundation logistic growth rate">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>2.41</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>2.43</eqn>
				</element>
				<units>1/metre</units>
			</aux>
			<aux name="Population inundation max fraction">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>0.36</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>0.2</eqn>
				</element>
				<units>1</units>
			</aux>
			<stock name="Retreated fraction\nof original coastal\npopulation distribution">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Initial_inundated_and_retreated_people_fraction[Coastal_Protection_level]</eqn>
				<inflow>relative_annual_proactive_retreat_of_people</inflow>
				<units>1</units>
			</stock>
			<flow name="relative annual proactive\nretreat of people">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MAX(0,  susceptible_fraction_willing_to_retreat - Retreated_fraction_of_original_coastal_population_distribution) / Proactive_retreat_time_scale</eqn>
				<units>1/year</units>
			</flow>
			<aux name="Retreat switch" access="input">
				<eqn>0.25 {0, 1}</eqn>
				<units>1</units>
			</aux>
			<aux name="Proactive retreat time scale" access="input">
				<eqn>20</eqn>
				<units>year</units>
			</aux>
			<flow name="Reactive retreat of\npeople from the coast" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Coastal_population* MAX(0, (Inundated_fraction_of_original_coastal_population_distribution + relative_annual_inundation_of_population_due_to_SLR*time_over_which_coastal_people_retreat) - Total_removed_fraction_of_original_population_distribution) / (1.0 - Total_removed_fraction_of_original_population_distribution) / time_over_which_coastal_people_retreat</eqn>
				<units>Mp/year</units>
			</flow>
			<aux name="Total removed\nfraction of original\npopulation distribution">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MAX(Inundated_fraction_of_original_coastal_population_distribution, Retreated_fraction_of_original_coastal_population_distribution)</eqn>
				<units>1</units>
			</aux>
			<aux name="Net global\nflood height" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>metre</units>
			</aux>
			<aux name="Change in net global\nflood height" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>metre</units>
			</aux>
			<aux name="Expected net global\nflood height in 50 years" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>metre</units>
			</aux>
			<aux name="time over which\ncoastal people retreat">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Actually susceptible people fraction">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>(Fraction_of_original_people_distribution_that_would_experience_storm_damage-Total_removed_fraction_of_original_population_distribution)/(1.0 - Total_removed_fraction_of_original_population_distribution)</eqn>
				<units>1</units>
			</aux>
			<aux name="Initial inundated and\nretreated people fraction">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>0.002</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>0.00059</eqn>
				</element>
				<units>1</units>
			</aux>
			<aux name="population storm\ndamage max fraction">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>0.52</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>0.39</eqn>
				</element>
				<units>1</units>
			</aux>
			<aux name="susceptible fraction willing to retreat">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Expected_susceptible_fraction_of_population_in_50_years*Retreat_switch</eqn>
				<units>1</units>
			</aux>
