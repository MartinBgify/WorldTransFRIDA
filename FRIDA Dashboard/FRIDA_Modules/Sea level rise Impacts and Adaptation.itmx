	<model name="Sea_level_rise_adaptation">
		<variables>
			<aux name="Annual\ncosts of\nflood protection\nmaintenance" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Average_flood_protection_construction_cost*Fraction_of_dike_construction_cost_for_maintenance_annually*Length_of_coastline[Coastal_Protection_level]*Average_flood_protection_height*Reference_fp_height_for_costs</eqn>
				<units>bc$/year</units>
			</aux>
			<stock name="Average flood\nprotection height" access="output" isee:autocreated="true">
				<doc>First implementation: set this to 0 for the 1980 level of protection. If sea level increases, the current flood protection level gets negative, i.e. the protection is worse</doc>
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Average_flood_protection_height_in_1980[Coastal_Protection_level]</eqn>
				<inflow>Increase_of_average_flood_protection_height_through_investment</inflow>
				<units>metre</units>
			</stock>
			<flow name="Increase of average\nflood protection height\nthrough investment" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>(SQRT(Average_flood_protection_height^2 + Effective_annual_investment_in_flood_protection_height*Time_for_change_in_average_fp_height/(Length_of_coastline[Coastal_Protection_level]*Average_flood_protection_construction_cost)) - Average_flood_protection_height)/Time_for_change_in_average_fp_height</eqn>
				<units>metre/year</units>
			</flow>
			<aux name="Fraction of dike construction cost for maintenance annually">
				<eqn>0.02</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Maximum amount of money spent\non flood protection annually">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>GDP_in_coastal_zones * Maximum_fraction_of_coastal_GDP_spent_for_flood_protection_annually</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Annual opportunity costs of flood protection" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Length_of_coastline[Coastal_Protection_level]*reference_opportunity_cost_rate*Value_of_land_in_the_coastal_zone*(Average_flood_protection_height-INIT(Average_flood_protection_height))*1.7*kilometre_per_metre</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Average flood protection\nconstruction cost">
				<doc>Made dependent on construction cost index as in CIAM</doc>
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>reference_protection_cost*Flood_protection_construction_cost_index</eqn>
				<units>bc$/metre/metre/km</units>
			</aux>
			<aux name="Average flood protection\nheight in 1980" access="output" isee:autocreated="true">
				<doc>Average flood protection height in DIVA for all segments with fp &gt; 0</doc>
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>1.46</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>2.8</eqn>
				</element>
				<units>metre</units>
			</aux>
			<aux name="Cost of reaching\ndesired protection">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MAX(0,  Average_flood_protection_construction_cost * Length_of_coastline[Coastal_Protection_level] * 
((Missing_protection_that_people_are_willing_to_build+Average_flood_protection_height)^2 - Average_flood_protection_height^2))</eqn>
				<units>bc$</units>
			</aux>
			<aux name="Time to increase flood protection height">
				<eqn>10</eqn>
				<units>year</units>
			</aux>
			<aux name="Effective annual\ninvestment in flood protection height" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MIN(Cost_of_reaching_desired_protection/Time_to_increase_flood_protection_height,  Money_available_for_new_flood_protection_investments)</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="kilometre per metre">
				<eqn>0.001</eqn>
				<units>km/metre</units>
			</aux>
			<aux name="Reference fp\nheight for costs" access="output" isee:autocreated="true">
				<eqn>1</eqn>
				<units>metre</units>
			</aux>
			<aux name="Time for change in average fp height">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Willingness to invest\nin flood protection 1" access="input">
				<eqn>0.25 {0, 1}</eqn>
				<units>1</units>
			</aux>
			<aux name="Maximum fraction\nof coastal GDP spent for flood\nprotection annually">
				<eqn>0.05</eqn>
				<units>1/year</units>
			</aux>
			<aux name="GDP in\ncoastal zones">
				<doc>GDP_per_capita_in_coastal_zones_in_1980*INIT(Coastal_population.Coastal_population) * (Coastal_Assets.Coastal_assets * Global_gdp_to_asset_ratio[Run]) / ( INIT(Coastal_Assets.Coastal_assets)*INIT(Global_gdp_to_asset_ratio[Run]) )</doc>
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Coastal_assets*Global_gdp_to_asset_ratio[Run]</eqn>
				<units>bc$</units>
			</aux>
			<aux name="Missing protection to\nreach perfect protection\nunder expected SLR">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Net_global_flood_height+Expected_global_SLR_in_50_years[Run]</eqn>
				<units>metre</units>
			</aux>
			<aux name="Average value of land in coastal zones in 1980">
				<eqn>0.005367</eqn>
				<units>bc$/km^2</units>
			</aux>
			<stock name="Coastal assets" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$</units>
			</stock>
			<stock name="Coastal\npopulation" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp</units>
			</stock>
			<aux name="GDP per capita\nin coastal zones" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>GDP_in_coastal_zones/Coastal_population</eqn>
				<units>bc$/Mp</units>
			</aux>
			<aux name="flood protection\ncost index sensitivity to GDPpc">
				<eqn>0.036853</eqn>
				<units>Mp / bc$</units>
			</aux>
			<aux name="reference\nprotection cost">
				<doc>From CIAM (Wong et al., 2022)

CIAM value : 0.00602

For the beginning this is held constant in the absence of other information. Unit should theoretically be bc$ per km coastline per m vertical elevation, but equation needs the &quot;m&quot; to be squared ?!

As described in CIAM documentation (https://github.com/delavane/CIAM) with reference to Hillen et al.: 

Hillen M, Jonkman S, Kanning W, Kok M, Geldenhuys M, Stive M (2010) Coastal defence cost estimates: Case study of the Netherlands, New Orleans and Vietnam. Communications on Hydraulic
and Geotechnical Engineering 1
</doc>
				<eqn>0.00602</eqn>
				<units>bc$/metre/metre/km</units>
			</aux>
			<aux name="flood protection\ncost index baseline">
				<eqn>0.3786</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Flood protection\nconstruction cost index">
				<doc>Fitted CCI to GDP using CIAM / DIVA data</doc>
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MIN(2.5,  MAX(0.5,  flood_protection_cost_index_baseline + GDP_per_capita_in_coastal_zones*flood_protection_cost_index_sensitivity_to_GDPpc))</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Reference maintenance costs without adaptation" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Average_flood_protection_construction_cost*Fraction_of_dike_construction_cost_for_maintenance_annually*Length_of_coastline[Coastal_Protection_level]*Average_flood_protection_height_in_1980[Coastal_Protection_level]*Reference_fp_height_for_costs</eqn>
				<units>bc$/year</units>
			</aux>
			<stock name="Land appreciation parameter">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>1</eqn>
				<inflow>Increase_of_value_of_land_in_coastal_zone</inflow>
				<units>1</units>
			</stock>
			<flow name="Increase of value of\nland in coastal zone">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>(EXP(Land_value_sensitivity_to_GDP_per_capita * TREND(GDP_per_capita_in_coastal_zones, Time_for_change_in_average_fp_height)+ Land_value_sensitivity_to_population * TREND(Coastal_population, 1) ) - 1) *Land_appreciation_parameter / Time_for_change_in_average_fp_height</eqn>
				<units>1/year</units>
			</flow>
			<aux name="Land value sensitivity to GDP per capita">
				<eqn>0.565</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Land value sensitivity to population">
				<eqn>0.313</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Potential maximum investment in flood protection over next 50 years">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Effective_annual_investment_in_flood_protection_height * Time_horizon</eqn>
				<units>bc$</units>
			</aux>
			<aux name="Time horizon">
				<eqn>50</eqn>
				<units>year</units>
			</aux>
			<aux name="Potential increase of flood\nprotection height\nover next 50 years" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>SQRT(Average_flood_protection_height^2 + Potential_maximum_investment_in_flood_protection_over_next_50_years/(Length_of_coastline[Coastal_Protection_level]*Average_flood_protection_construction_cost)) - Average_flood_protection_height</eqn>
				<units>metre</units>
			</aux>
			<aux name="Money available for new flood protection investments">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MAX(0,  Maximum_amount_of_money_spent_on_flood_protection_annually - Annual_costs_of_flood_protection_maintenance)</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Flood protection\nheight change since 1980" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Average_flood_protection_height-INIT(Average_flood_protection_height)</eqn>
				<units>metre</units>
			</aux>
			<aux name="reference opportunity cost rate">
				<doc>as in CIAM</doc>
				<eqn>0.04</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Value of land in the coastal zone">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Land_appreciation_parameter*Average_value_of_land_in_coastal_zones_in_1980</eqn>
				<units>bc$/km^2</units>
			</aux>
			<aux name="Inundated or abandoned coastal\narea">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>coastal_inundation_max_area[Coastal_Protection_level] / (1.0 + EXP(-coastal_area_inundation_logistic_growth_rate[Coastal_Protection_level]*(MAX(Net_global_flood_height, Effective_retreat_height) - coastal_area_inundation_halfpoint[Coastal_Protection_level]) ) )</eqn>
				<units>km^2</units>
			</aux>
			<aux name="coastal area inundation halfpoint">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>2.25</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>2.53</eqn>
				</element>
				<units>metre</units>
			</aux>
			<aux name="coastal area inundation\nlogistic growth rate">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>2.02</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>2.74</eqn>
				</element>
				<units>1/metre</units>
			</aux>
			<aux name="coastal inundation max area">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>1604403.3</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>459985.2</eqn>
				</element>
				<units>km^2</units>
			</aux>
			<aux name="Length of coastline">
				<doc>Data from DIVA</doc>
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>662908.5</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>375325.4</eqn>
				</element>
				<units>km</units>
			</aux>
			<aux name="Annual opportunity costs of\ninundated or abandoned coastal land" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Value_of_land_in_the_coastal_zone*Inundated_or_abandoned_coastal_area*reference_opportunity_cost_rate</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Expected net global\nflood height in 50 years" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>MAX(0,  Net_global_flood_height+ Expected_global_SLR_in_50_years[Run] - Potential_increase_of_flood_protection_height_over_next_50_years)</eqn>
				<units>metre</units>
			</aux>
			<aux name="Net global\nflood height" access="output" isee:autocreated="true">
				<doc>The effective flood height simply increases linearly with global SLR</doc>
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>global_coastal_SLR_since_1980 [Run,Coastal_Protection_level]- Flood_protection_height_change_since_1980</eqn>
				<units>metre</units>
			</aux>
			<aux name="global coastal SLR since 1980">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>IF (switch_sea_level_impact =0) AND TIME &gt; today THEN
PREVIOUS(SELF, 0)
ELSE
(Total_global_SLR[Run]- INIT(Total_global_SLR[Run])) * coastal_SLR_factors[Coastal_Protection_level]</eqn>
				<units>metre</units>
			</aux>
			<aux name="coastal SLR factors">
				<dimensions>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<element subscript="insufficient">
					<eqn>1.0382</eqn>
				</element>
				<element subscript="well_protected">
					<eqn>0.9298</eqn>
				</element>
				<units>1</units>
			</aux>
			<aux name="Change in net global\nflood height" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Net_global_flood_height - DELAY(Net_global_flood_height,  1)</eqn>
				<units>metre</units>
			</aux>
			<aux name="Effective retreat height" access="input">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>metre</units>
			</aux>
			<aux name="Missing protection that people are willing to build">
				<dimensions>
					<dim name="Run"/>
					<dim name="Coastal_Protection_level"/>
				</dimensions>
				<eqn>Missing_protection_to_reach_perfect_protection_under_expected_SLR*Willingness_to_invest_in_flood_protection_1</eqn>
				<units>metre</units>
			</aux>
			<aux name="Global gdp to asset ratio">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>&quot;Real_GDP_in_2021c$&quot;/(Financial_bank_assets_in_real_terms/Time_for_change_in_average_fp_height)</eqn>
				<units>1</units>
			</aux>
			<aux name="today" access="input">
				<eqn>2025</eqn>
				<units>year</units>
			</aux>
			<aux name="switch sea level impact" access="input">
				<eqn>1</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Real GDP in 2021c$" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/year</units>
			</aux>
			<aux name="Financial bank\nassets in real terms" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$</units>
			</aux>
